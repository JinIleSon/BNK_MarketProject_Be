<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.bnk_marketproject_be.mapper.CompInfoMapper">

    <select id="selectFive" resultType="kr.co.bnk_marketproject_be.dto.CompInfoDTO">
        SELECT *
        FROM (
        SELECT *
        FROM compinfo
        ORDER BY created_at DESC
        )
        WHERE ROWNUM &lt;= 5
    </select>

    <select id="selectAll" resultType="kr.co.bnk_marketproject_be.dto.CompInfoDTO">
        SELECT * FROM compinfo ORDER BY CREATED_AT DESC
    </select>
</mapper>